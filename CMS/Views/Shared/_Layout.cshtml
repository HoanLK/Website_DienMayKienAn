<!DOCTYPE html>
<html lang="vi" ⚡="" amp-version="2003101714470" class="i-amphtml-singledoc i-amphtml-standalone">
<head>
    <!-- Basic -->
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta http-equiv="Content-Language" content="VN">

    <title>@Html.Raw(ViewBag.title) | @Html.Raw("Điện máy Kiến An")</title>
    <meta name="keywords" content="@Html.Raw(ViewBag.keywords)" />
    <meta name="description" content="@Html.Raw(ViewBag.description)" />
    <link rel="canonical" href="@Html.Raw(ViewBag.url)" />

    <meta name="url" content="@Html.Raw(ViewBag.url)" />
    <meta name="identifier-URL" content="@Html.Raw(ViewBag.url)" />
    <meta name="coverage" content="Worldwide" />
    <meta name="distribution" content="Global" />
    <meta name="rating" content="General" />
    <meta name="revisit-after" content="7 days" />
    <meta http-equiv="Expires" content="0" />

    <!-- OpenGraph Meta Tags -->
    <meta name="og:title" content="@Html.Raw(ViewBag.title)" />
    <meta name="og:url" content="@Html.Raw(ViewBag.url)" />
    <meta name="og:image" content="@Html.Raw(ViewBag.image)" />
    <meta name="og:image:secure_url" content="@Html.Raw(ViewBag.image)" />
    <meta name="og:description" content="@Html.Raw(ViewBag.description)" />

    <!-- FIXED -->
    <meta name="og:postal-code" content="180000" />
    <meta name="og:country-name" content="VietNam" />

    <meta name="twitter:card" content="summary_large_image" />
    <meta name="twitter:description" content="@Html.Raw(ViewBag.description)" />
    <meta name="twitter:title" content="@Html.Raw(ViewBag.title)" />

    <!-- FIX META -->
    @Html.Action("HeaderFixed", "Layout")

    <!-- ::::::::::::::All CSS Files here :::::::::::::: -->
    @Styles.Render("~/css")

</head>
<body ng-app="frontendApp" ng-controller="MainController as main">
    <!-- ::::::  HEADER  ::::::  -->
    @Html.Action("Header", "Layout")

    <!-- ::::::  MAIN  ::::::  -->
    @RenderBody()

    <!-- ::::::  FOOTER  ::::::  -->
    @Html.Action("Footer", "Layout")

    <!-- Modal By Now -->
    @Html.Partial("~/Views/Shared/_BuyNowModal.cshtml")

    <!-- Material-Scrolltop Button -->
    <button class="material-scrolltop" type="button"></button>

    <!-- Mobile Call -->
    <p class="mobile_support">
        <i class="fas fa-phone"></i>
        <a href="tel:0393785785">
            0393 785 785
        </a>
    </p>

    <!-- ::::::::::::::All Javascripts Files here ::::::::::::::-->
    @Scripts.Render("~/js")
    @Scripts.Render("~/angularjs")

    @RenderSection("scripts", required: false)
</body>
</html>
