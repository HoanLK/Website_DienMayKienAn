<!DOCTYPE html>
<html lang="vi" ⚡="" amp-version="2003101714470" class="i-amphtml-singledoc i-amphtml-standalone">
<head>
    <!-- Basic -->
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta http-equiv="Content-Language" content="vi">

    <title>@Html.Raw(ViewBag.title) | @Html.Raw("Điện máy Kiến An")</title>
    <meta name="keywords" content="@Html.Raw(ViewBag.keywords)" />
    <meta name="description" content="@Html.Raw(ViewBag.description)" />
    <link rel="canonical" href="@Html.Raw(ViewBag.url)" />

    <meta name="url" content="@Html.Raw(ViewBag.url)" />
    <meta name="identifier-URL" content="@Html.Raw(ViewBag.url)" />
    <meta name="coverage" content="Worldwide" />
    <meta name="distribution" content="Global" />
    <meta name="rating" content="General" />
    <meta name="revisit-after" content="7 days" />
    <meta http-equiv="Expires" content="0" />

    <!-- OpenGraph Meta Tags -->
    <meta name="og:title" content="@Html.Raw(ViewBag.title)" />
    <meta name="og:url" content="@Html.Raw(ViewBag.url)" />
    <meta name="og:image" content="@Html.Raw(ViewBag.image)" />
    <meta name="og:image:secure_url" content="@Html.Raw(ViewBag.image)" />
    <meta name="og:description" content="@Html.Raw(ViewBag.description)" />

    <!-- FIXED -->
    <meta name="og:postal-code" content="180000" />
    <meta name="og:country-name" content="VietNam" />

    <meta name="twitter:card" content="summary_large_image" />
    <meta name="twitter:description" content="@Html.Raw(ViewBag.description)" />
    <meta name="twitter:title" content="@Html.Raw(ViewBag.title)" />

    <!-- FIX META -->
    @Html.Action("HeaderFixed", "Layout")

    <!-- ::::::::::::::All CSS Files here :::::::::::::: -->
    @Styles.Render("~/css")
    <!-- Fonts -->
    <link rel="preload" href="/fonts/fontAwesome5Pro/fa-regular-400.woff2" as="font" type="font/woff2" crossorigin>
    <link rel="preload" href="/fonts/fontAwesome5Pro/fa-solid-900.woff2" as="font" type="font/woff2" crossorigin>
    <link rel="preload" href="/fonts/plaza-icon/plaza-icon.ttf?uto3fr" as="font" type="font/woff2" crossorigin>
</head>
<body ng-app="frontendApp" ng-controller="MainController as main">
    <!-- Load Facebook SDK for JavaScript -->
    <div id="fb-root"></div>
    <script>
        window.fbAsyncInit = function () {
            FB.init({
                xfbml: true,
                version: 'v8.0'
            });
        };

        (function (d, s, id) {
            var js, fjs = d.getElementsByTagName(s)[0];
            if (d.getElementById(id)) return;
            js = d.createElement(s); js.id = id;
            js.src = 'https://connect.facebook.net/vi_VN/sdk/xfbml.customerchat.js';
            fjs.parentNode.insertBefore(js, fjs);
        }(document, 'script', 'facebook-jssdk'));</script>

    <!-- Your Chat Plugin code -->
    <div class="fb-customerchat"
         attribution=setup_tool
         page_id="115978803527376"
         theme_color="#0063d1"
         logged_in_greeting="Xin chào! Chúng tôi có thể hỗ trợ gì cho bạn không?"
         logged_out_greeting="Xin chào! Chúng tôi có thể hỗ trợ gì cho bạn không?">
    </div>

    <!-- ::::::  HEADER  ::::::  -->
    @Html.Action("Header", "Layout")

    <!-- ::::::  MAIN  ::::::  -->
    @RenderBody()

    <!-- ::::::  FOOTER  ::::::  -->
    @Html.Action("Footer", "Layout")

    <!-- Modal By Now -->
    @Html.Partial("~/Views/Shared/_BuyNowModal.cshtml")
    <!-- Modal Buy Success -->
    @Html.Partial("~/Views/Shared/_BuySuccessModal.cshtml")

    <!-- Material-Scrolltop Button -->
    <button class="material-scrolltop" style="bottom: 95px; right: 35px;" type="button"></button>

    <!-- Mobile Call -->
    <p class="mobile_support">
        <i class="fas fa-phone"></i>
        <a href="tel:0393785785">
            0393 785 785
        </a>
    </p>

    <!-- ::::::::::::::All Javascripts Files here ::::::::::::::-->
    @Scripts.Render("~/js")
    @Scripts.Render("~/angularjs")

    @RenderSection("scripts", required: false)
</body>
</html>
